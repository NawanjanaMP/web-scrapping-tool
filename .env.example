# ===========================
# .env.example
# ===========================

# Application Settings
APP_NAME=Web Scraping Tool
ENVIRONMENT=development
SECRET_KEY=your-secret-key-here

# Server Settings
HOST=0.0.0.0
PORT=8000

# Scraping Settings
REQUEST_TIMEOUT=30
MAX_RETRIES=3
USER_AGENT=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36

# Rate Limiting
RATE_LIMIT_PER_MINUTE=60

# Security
MAX_URL_LENGTH=2048